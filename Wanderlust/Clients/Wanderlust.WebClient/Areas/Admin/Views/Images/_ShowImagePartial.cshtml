@model Wanderlust.WebClient.Areas.Admin.Models.ImageViewModel
@{
    ViewBag.Title = "LikeImage";
}

<div class="likes-section" id="@Model.Id">
    @if (Model.HasBeenHidden)
    {
        <button class="btn btn-like" name="hideImg" value="Show">Show to users</button>
    }
    else
    {
        <button class="btn btn-like" name="hideImg" value="Hide">Hide from users</button>
    }
</div>



     

  

